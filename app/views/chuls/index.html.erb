<p id="notice"><%= notice %></p>

<h1>Chuls</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chuls.each do |chul| %>
      <tr>
        <td><%= chul.title %></td>
        <td><%= chul.content %></td>
        <td><%= link_to 'Show', chul %></td>
        <td><%= link_to 'Edit', edit_chul_path(chul) %></td>
        <td><%= link_to 'Destroy', chul, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Chul', new_chul_path %>
